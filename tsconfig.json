{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "incremental": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "resolveJsonModule": true
  }
}
